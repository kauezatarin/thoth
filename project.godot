; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Thoth",
"language": "GDScript",
"path": "res://scripts/thoth.gd"
}, {
"base": "Node",
"class": "ThothAssembler",
"language": "GDScript",
"path": "res://addons/thoth/thoth_assembler.gd"
}, {
"base": "Node",
"class": "ThothGameState",
"language": "GDScript",
"path": "res://addons/thoth/thoth_gamestate.gd"
}, {
"base": "Node",
"class": "ThothSerializer",
"language": "GDScript",
"path": "res://addons/thoth/thoth_serializer.gd"
} ]
_global_script_class_icons={
"Thoth": "",
"ThothAssembler": "",
"ThothGameState": "",
"ThothSerializer": ""
}

[application]

config/name="Thoth"
run/main_scene="res://scenes/main.tscn"
config/icon="res://textures/icon.png"

[display]

window/size/width=1920
window/size/height=1080

[editor_plugins]

enabled=PoolStringArray( "res://addons/thoth/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
